{
  "name": "web-api",
  "version": "0.0.1",
  "description": "Web Application Starter",
  "author": "Deojeff Lai",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "NODE_ENV=production nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "malaysia:dev": "COUNTRY=malaysia yarn start:dev malaysia",
    "singapore:dev": "COUNTRY=singapore yarn start:dev singapore",
    "malaysia:migrate:make": "COUNTRY=malaysia yarn knex migrate:make",
    "malaysia:migrate:up": "COUNTRY=malaysia yarn knex migrate:latest",
    "malaysia:migrate:down": "COUNTRY=malaysia yarn knex migrate:rollback",
    "knex": "TS_NODE_PROJECT=tsconfig.json knex --knexfile ./libs/shared/src/database/knexfile.ts",
    "migrate": "yarn knex migrate:latest",
    "rollback": "yarn knex migrate:rollback --all",
    "seed": "yarn knex seed:run",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/web-api/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.0.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/schedule": "^0.4.3",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/swagger": "^4.7.12",
    "@nestjs/throttler": "^1.1.1",
    "@willsoto/nestjs-objection": "^4.0.2",
    "aws-sdk": "^2.856.0",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^3.4.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-sanitizer": "^1.0.1",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "firebase-admin": "^9.4.2",
    "helmet": "^4.6.0",
    "hpp": "^0.2.3",
    "http-link-header": "^1.0.3",
    "knex": "^0.21.17",
    "mailgun-js": "^0.22.0",
    "mysql": "^2.18.1",
    "nanoid": "^3.1.20",
    "objection": "^2.2.14",
    "objection-paginator": "^0.6.6",
    "passport": "^0.4.1",
    "passport-http-bearer": "^1.0.1",
    "pg": "^8.6.0",
    "pug": "^3.0.0",
    "rand-token": "^1.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "slugify": "^1.4.7",
    "stripe": "^8.148.0",
    "to-snake-case": "^1.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cache-manager": "^3.4.0",
    "@types/compression": "^1.7.0",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.3",
    "@types/hpp": "^0.2.1",
    "@types/http-link-header": "^1.0.2",
    "@types/jest": "25.2.3",
    "@types/mailgun-js": "^0.22.11",
    "@types/node": "^13.9.1",
    "@types/passport": "^1.0.6",
    "@types/passport-http-bearer": "^1.0.36",
    "@types/pug": "^2.0.4",
    "@types/stripe": "^8.0.417",
    "@types/supertest": "^2.0.8",
    "@types/to-snake-case": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.0.1",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@libs/libs/(.*)": "<rootDir>/libs/libs/src/$1",
      "@libs/libs": "<rootDir>/libs/libs/src",
      "@libs/shared/(.*)": "<rootDir>/libs/shared/src/$1",
      "@libs/shared": "<rootDir>/libs/shared/src"
    }
  }
}
